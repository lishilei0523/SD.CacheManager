<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!--SD.CacheManager配置节点-->
    <section name="sd.cacheManager" type="SD.CacheManager.CacheManagerSection, SD.CacheManager" />
  </configSections>

  <!--SD.CacheManager配置-->
  <sd.cacheManager>
    <!--缓存提供者，本地内存-->
    <cacheProvider type="SD.CacheManager.MemoryCacheProvider" assembly="SD.CacheManager.MemoryCache" />
    <!--缓存提供者，Redis-->
    <cacheProvider type="SD.CacheManager.RedisProvider" assembly="SD.CacheManager.Redis" />
    <!--缓存提供者，Memcached-->
    <cacheProvider type="SD.CacheManager.MemcachedProvider" assembly="SD.CacheManager.Memcached" />
  </sd.cacheManager>
</configuration>
